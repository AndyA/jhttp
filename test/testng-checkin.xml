<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="jhttp-pre-checkin-tests" verbose="2" parallel="methods">
    <test name="pre-checkin">
        <groups/>
        <packages>
            <package name="net.jhttp"/>
        </packages>
        <run>
            <include name="checkin"/>
            <exclude name="broken"/>
        </run>
    </test>
</suite>
